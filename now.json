{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/resume",
      "status": 302,
      "headers": {
        "Location": "/AndreDuvoisinResume.pdf"
      }
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "404.html" }
  ]
}
